<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		{# Run `composer require symfony/webpack-encore-bundle`
				           and uncomment the following Encore helpers to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>

	<body>
		<div class="container">
			{{include("menu.html.twig")}}

			{% for type,messages in app.flashes %}

				{% for msg in messages %}
					<div class="alert alert-{{ type }}">
						{{ msg }}
					</div>
				{% endfor %}

			{% endfor %}

			<h1>
				{% block titreH1 %}
					Bienvenue sur notre ecommerce wf3
				{% endblock %}
			</h1>

			{% block body %}
				page d'accueil
				<p>Prenom =
					{{prenom }}</p>
			{% endblock %}

			{% block footer %}
				<br>
				<footer>

				

				</footer>
			{% endblock %}

		</div>
	</body>
</html>
